      <seq n="num_trencat"/>
      <seq n="mitja_comunicacio"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
    <or>
      <seq n="lloc_concret"/>
      <seq n="territori"/>
      <seq n="accident_geogr"/>
      <seq n="part_ciutat"/>
      <seq n="periode"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
    <match tags="det.*"/>
    <match tags="adj.*"/>
    <or>
      <seq n="punt_cardinal"/>
      <seq n="lloc_concret"/>
      <seq n="lloc_abstracte"/>
      <seq n="territori"/>
      <seq n="accident_geogr"/>
      <seq n="baralla"/>
      <seq n="part_ciutat"/>
      <seq n="escrit"/>
      <seq n="període"/>
      <seq n="part_cos"/>
      <seq n="vestit"/>
      <seq n="mes"/>
      <seq n="estacio"/>
      <seq n="num_trencat"/>
      <seq n="mitja_comunicacio"/>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="carta" tags="n.*"/>
      <match lemma="missatge" tags="n.*"/>
      <match lemma="salutació" tags="n.*"/>
      <match lemma="epístola" tags="n.*"/>
      <match lemma="ordre" tags="n.f.*"/>
      <match lemma="comiat" tags="n.*"/>
      <match lemma="traducció" tags="n.*"/>
    </or>
    <repeat from="0" upto="1">
      <or><match tags="adj.*"/><match tags="vblex.pp.*"/></or>
    </repeat>
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="traducció" tags="n.*"/>
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="traducció" tags="n.*"/>
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
    <match tags="det.*"/>
    <match tags="n.*"/>
    <match lemma="de" tags="pr"><select lemma="di" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="traducció" tags="n.*"/>
    <match lemma="de" tags="pr"/>
    <match tags="det.*"/>
    <match tags="n.*"/>
    <repeat from="0" upto="1">
      <match tags="adj.*"/>
    </repeat>
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="ser" tags="vbser.*"/>
      <match lemma="estar" tags="vblex.*"/>
      <match lemma="estudiar" tags="vblex.*"/>
