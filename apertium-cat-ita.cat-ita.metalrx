  <!-- edat mitjana >> medioevo, età media -->
  <rule weight="0.8">
    <match lemma="edat mitjana" tags="n.*"><select lemma="medioevo"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="edat mitjana" tags="n.*"><select lemma="età media"/></match>
    <match lemma="de" tags="pr"/>
  </rule>

  <!-- vestit >> robe, costume --> <!-- cat > fra -->
  <rule weight="0.8">
    <match lemma="vestit" tags="n.*"><select lemma="robe"/></match>
  </rule>

  <!-- volt >> tour, volt --> <!-- cat > fra -->
  <rule weight="0.8">
    <match lemma="volt" tags="n.*"><select lemma="tour"/></match>
  </rule>
  <rule weight="1.0">
    <or><match tags="num.*"/><match tags="num"/></or>
    <match lemma="volt" tags="n.*"><select lemma="volt"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="volt" tags="n.*"><select lemma="tour"/></match>
    <or>
      <match lemma="a" tags="pr"/>
      <match lemma="per" tags="pr"/>
    </or>
  </rule>

  <!-- got > goto, bicchiere -->
  <rule weight="0.6">
     <match lemma="got" tags="n.*"><select lemma="goto"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="got" tags="n.*"><select lemma="bicchiere"/></match>
     <match lemma="de" tags="pr"/>
  </rule>
  <rule weight="1.2">
     <match lemma="got" tags="n.*"><select lemma="goto"/></match>
     <match lemma="de" tags="pr"/>
     <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.hyd.*"/></or>
  </rule>

  <!-- guàrdia > guardia, poliziotto -->
  <rule weight="0.6">
     <match lemma="guàrdia" tags="n.*"><select lemma="guardia"/></match>
  </rule>
  <rule weight="0.6">
     <match tags="det.*.m.*"/>
     <match lemma="guàrdia" tags="n.*"><select lemma="poliziotto"/></match>
  </rule>

  <!-- xic (n) > pitzocu, pagu --> <!-- srd -->
  <rule weight="0.6">
     <match lemma="xic" tags="n.m.*"><select lemma="pitzocu"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="xic" tags="n.m.*"><select lemma="pagu"/></match>
     <match lemma="de" tags="pr"/>
  </rule>
  <rule weight="1.2">
     <match lemma="xic" tags="n.m.*"><select lemma="pitzocu"/></match>
     <match lemma="de" tags="pr"/>
     <match tags="np.*"/>
  </rule>

  <!-- cap >> capo, testa -->
  <rule weight="0.8">
    <match lemma="cap" tags="n.*"><select lemma="capo"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="capo"/></match>
    <or><match tags="np.top.*"/><match tags="np.loc"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="capo"/></match>
    <match lemma="de" tags="pr"/>
    <or><match tags="np.top.*"/><match tags="np.loc"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="cap" tags="n.*"><select lemma="testa"/></match>
    <match lemma="de" tags="pr"/>
    <or>
      <match lemma="comarca" tags="n.*"/>

      <match lemma="animal" tags="n.*"/>
      <match lemma="peix" tags="n.*"/>
      <match lemma="gos" tags="n.*"/>
      <match lemma="gat" tags="n.*"/>
      <match lemma="bou" tags="n.*"/>
      <match lemma="vaca" tags="n.*"/>
    </or>
  </rule>

  <!-- població > popolazione, centro abitato -->
  <rule weight="0.6">
     <match lemma="població" tags="n.*"><select lemma="popolazione"/></match>
  </rule>

  <!-- caragol >> chiocciola, vite -->
  <rule weight="0.8">
    <match lemma="caragol" tags="n.*"><select lemma="chiocciola"/></match>
  </rule>
  <rule weight="0.8">
    <match lemma="cargol" tags="n.*"><select lemma="chiocciola"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="caragol" tags="n.m.*"><select lemma="vite"/></match>
     <or>
       <match lemma="pla" tags="adj.*"/>
       <match lemma="quadrat" tags="adj.*"/>
       <match lemma="hexagonal" tags="adj.*"/>
     </or>
  </rule>
  <rule weight="1.0">
     <match lemma="caragol" tags="n.m.*"><select lemma="vite"/></match>
     <match lemma="en" tags="pr"/>
     <match lemma="estrella" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="caragol" tags="n.m.*"><select lemma="vite"/></match>
     <match lemma="de" tags="pr"/>
     <match lemma="pala" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="caragol" tags="n.m.*"><select lemma="vite"/></match>
     <or><match tags="np.cog.*"/><match tags="np.al.*"/></or> <!-- Philips, Allen, etc. -->
  </rule>
  <rule weight="1.0">
     <match lemma="cargol" tags="n.m.*"><select lemma="vite"/></match>
     <or>
       <match lemma="pla" tags="adj.*"/>
       <match lemma="quadrat" tags="adj.*"/>
       <match lemma="hexagonal" tags="adj.*"/>
     </or>
  </rule>
  <rule weight="1.0">
     <match lemma="cargol" tags="n.m.*"><select lemma="vite"/></match>
     <match lemma="en" tags="pr"/>
     <match lemma="estrella" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="cargol" tags="n.m.*"><select lemma="vite"/></match>
     <match lemma="de" tags="pr"/>
     <match lemma="pala" tags="n.*"/>
  </rule>
  <rule weight="1.0">
     <match lemma="cargol" tags="n.m.*"><select lemma="vite"/></match>
     <or><match tags="np.cog.*"/><match tags="np.al.*"/></or> <!-- Philips, Allen, etc. -->
  </rule>

  <!-- tret > caratteristica, sparo -->
  <rule weight="0.6">
     <match lemma="tret" tags="n.*"><select lemma="caratteristica"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="tret" tags="n.*"><select lemma="sparo"/></match>
     <match lemma="a" tags="pr"/>
  </rule>

  <!-- taula > tavola, tavolo, tabella -->
  <rule weight="0.6">
     <match lemma="taula" tags="n.*"><select lemma="tavola"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="taula" tags="n.*"><select lemma="tavola"/></match>
     <match lemma="de" tags="pr"/>
     <or>
       <match lemma="multiplicar" tags="n.*"/>
       <match lemma="dada" tags="n.*"/>
       <match lemma="còmput" tags="n.*"/>
       <match lemma="ordenació" tags="n.*"/>
       <match lemma="cas" tags="n.*"/>
       <match lemma="símbol" tags="n.*"/>
       <match lemma="literal" tags="n.*"/>
       <match lemma="Pitàgores" tags="np.*"/>
       <match lemma="Mendeléiev" tags="np.*"/>
       <match lemma="Huffman" tags="np.*"/>
     </or>
  </rule>
  <rule weight="1.0">
     <match lemma="taula" tags="n.*"><select lemma="tavola"/></match>
     <or>
       <match lemma="periòdic" tags="adj.*"/>
       <match lemma="astronòmic" tags="adj.*"/>
       <match lemma="cronològic" tags="adj.*"/>
       <match lemma="següent" tags="adj.*"/>
       <match lemma="anterior" tags="adj.*"/>
       <match lemma="superior" tags="adj.*"/>
       <match lemma="inferior" tags="adj.*"/>
       <match lemma="esquemàtica" tags="adj.*"/>
       <match lemma="número"/>
       <match lemma="núm."/>
       <match lemma="no."/>
       <match tags="num"/>
     </or>
  </rule>


<!-- **************** NP **************** -->

<!-- Virgili > Virgili, Virgilio -->
  <rule weight="0.6">
     <match lemma="Virgili" tags="np.*"><select lemma="Virgilio"/></match>
  </rule>
  <rule weight="1.0">
     <or><match tags="np.ant.*"/><match tags="np.cog.*"/></or>
     <match lemma="Virgili" tags="np.*"><select lemma="Virgili"/></match>
  </rule>
  <rule weight="1.0">
     <or><match tags="np.cog.*"/><match lemma="Rovira"/></or>
     <match lemma="i" tags="pr"/>
     <match lemma="Virgili" tags="np.*"><select lemma="Virgili"/></match>
  </rule>

<!-- Jesús > Gesù, Jesús -->
  <rule weight="0.6">
     <match lemma="Jesús" tags="np.*"><select lemma="Gesù"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="Jesús" tags="np.*"><select lemma="Jesús"/></match>
     <or><match tags="np.ant.*"/><match tags="np.cog.*"/></or>
  </rule>
