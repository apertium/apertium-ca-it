  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
    <match tags="predet.*"/>
    <match tags="det.*"/>
    <or>
      <seq n="punt_cardinal"/>
      <seq n="lloc_concret"/>
      <seq n="lloc_abstracte"/>
      <seq n="territori"/>
      <seq n="accident_geogr"/>
      <seq n="baralla"/>
      <seq n="part_ciutat"/>
      <seq n="escrit"/>
      <seq n="periode"/>
