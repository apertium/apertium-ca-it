<rules>

<!-- **************** DET **************** -->

  <!-- el meu > il mio, mio -->
  <rule weight="0.6">
     <match lemma="el meu" tags="det.*"><select lemma="il mio"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="el meu" tags="det.*"><select lemma="mio"/></match>
     <or>
       <seq n="familiar"/>
     </or>
  </rule>

  <!-- el teu > il tuo, tuo -->
  <rule weight="0.6">
     <match lemma="el teu" tags="det.*"><select lemma="il tuo"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="el teu" tags="det.*"><select lemma="tuo"/></match>
     <or>
       <seq n="familiar"/>
     </or>
  </rule>

  <!-- el nostre > il nostro, nostro -->
  <rule weight="0.6">
     <match lemma="el nostre" tags="det.*"><select lemma="il nostro"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="el nostre" tags="det.*"><select lemma="nostro"/></match>
     <or>
       <seq n="familiar"/>
     </or>
  </rule>

  <!-- el vostre > il vostro, vostro -->
  <rule weight="0.6">
     <match lemma="el vostre" tags="det.*"><select lemma="il vostro"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="el vostre" tags="det.*"><select lemma="vostro"/></match>
     <or>
       <seq n="familiar"/>
     </or>
  </rule>

  <!-- el seu > il suo, suo, il loro, loro -->
  <rule weight="0.6">
    <match lemma="el seu" tags="det.*"><select lemma="il suo" tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
     <match lemma="el seu" tags="det.*"><select lemma="suo"/></match>
     <or>
       <seq n="familiar"/>
     </or>
  </rule>

  <!-- cada > ogni, ciascun -->
  <rule weight="0.6">
    <match lemma="cada" tags="det.*"><select lemma="ogni" tags="det.*"/></match>
  </rule>

  <!-- altre > altro, un altro -->
  <rule weight="0.6">
    <match lemma="altre" tags="det.*"><select lemma="altro" tags="det.*"/></match>
  </rule>

  <!-- cap > nessuno, alcun -->
  <rule weight="0.6">
    <match lemma="cap" tags="det.*"><select lemma="nessuno" tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="sense" tags="pr"/>
    <match lemma="cap" tags="det.*"><select lemma="alcun" tags="det.*"/></match>
  </rule>
  <rule weight="1.0" c="no hi ha cap pressa">
    <match lemma="no" tags="adv"/>
     <repeat from="0" upto="2">
       <or>
         <match tags="prn.pro.*"/>
         <match tags="adv"/>
       </or>
     </repeat>
    <or><match tags="vblex.*"/><match tags="vbmod.*"/><match tags="vbhaver.*"/><match tags="vbser.*"/></or>
    <match lemma="cap" tags="det.*"><select lemma="alcun" tags="det.*"/></match>
  </rule>

<!-- **************** PR **************** -->

<!-- a > a, in, de, tra, fra -->
  <rule weight="0.6">
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
    <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.hyd.*"/><match tags="np.al"/><match tags="np.al.*"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
    <match tags="det.*"/>
    <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.hyd.*"/><match tags="np.al"/><match tags="np.al.*"/></or>
  </rule>
  <rule weight="1.0">
<!-- <match tags="vblex.*"/> -->
    <match lemma="de" tags="pr"><select lemma="di" tags="pr"/></match>
    <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.hyd.*"/><match tags="np.al"/><match tags="np.al.*"/></or>
    <match lemma="a" tags="pr"><select lemma="a" tags="pr"/></match>
    <or><match tags="np.loc"/><match tags="np.top.*"/><match tags="np.hyd.*"/><match tags="np.al"/><match tags="np.al.*"/></or>
  </rule>
  <rule weight="1.0">
    <match lemma="a" tags="pr"><select lemma="in" tags="pr"/></match>
    <match tags="det.*"/>
    <or>
      <seq n="punt_cardinal"/>
      <seq n="lloc_concret"/>
      <seq n="lloc_abstracte"/>
      <seq n="territori"/>
      <seq n="accident_geogr"/>
      <seq n="baralla"/>
