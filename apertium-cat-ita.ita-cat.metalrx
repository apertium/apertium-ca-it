  <!-- da > de, per, des de -->
  <rule weight="0.6">
    <match lemma="da" tags="pr"><select lemma="de" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="da" tags="pr"><select lemma="per" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <or><match tags="vblex.pp.*"/><match tags="vbmod.pp.*"/></or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="in" tags="pr"/>
    <match lemma="il" tags="det.def.*"/>
    <match tags="num"/>
    <match lemma="da" tags="pr"><select lemma="per" tags="pr"/></match>
  </rule>
  <rule weight="1.2">
    <match lemma="da" tags="pr"><select lemma="per" tags="des de"/></match>
    <match tags="det.def.m.sg"/>
    <match tags="num"/>
  </rule>

  <!-- tra, fra > entre, a -->
  <rule weight="0.6">
    <or>
      <match lemma="tra" tags="pr"><select lemma="entre" tags="pr"/></match>
      <match lemma="fra" tags="pr"><select lemma="entre" tags="pr"/></match>
    </or>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="tra" tags="pr"><select lemma="a" tags="pr"/></match>
      <match lemma="fra" tags="pr"><select lemma="a" tags="pr"/></match>
    </or>
    <or><match tags="num.*"/><match tags="det.ind.f.sg"/></or>
    <or>
      <seq n="distanza"/>
    </or>
  </rule>

  <!-- su > sobre, de, en -->
  <rule weight="0.6">
    <match lemma="su" tags="pr"><select lemma="sobre" tags="pr"/></match>
  </rule>
  <!-- due volte su cinque >> dues vegades DE cinc -->
  <rule weight="1.0">
    <or><match tags="num.*"/><match tags="det.ind.f.sg"/></or>
    <match lemma="volta" tags="n.*"/>
    <match lemma="su" tags="pr"><select lemma="de"/></match>
    <match tags="num.*"/>
  </rule>

  <!-- basarsi su >> basar-se en -->
  <rule weight="1.0">
    <match lemma="basare" tags="vblex.*"/>
    <repeat from="0" upto="1">
      <match tags="prn.enc.*"/>
    </repeat>
    <match lemma="su" tags="pr"><select lemma="en" tags="pr"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="basare" tags="vblex.*"/>
    <repeat from="0" upto="1">
      <match tags="prn.enc.*"/>
    </repeat>
    <match tags="adv"/>
    <match lemma="su" tags="pr"><select lemma="en" tags="pr"/></match>
  </rule>

  <!-- a >> a, en, de -->
  <rule weight="0.6">
    <match lemma="a" tags="pr"><select lemma="a"/></match>
  </rule>
  <rule weight="1.0">
    <or>
      <match lemma="pensare"/>
      <match lemma="partecipare"/>
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="a" tags="pr"><select lemma="en"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="addetto" tags="n.*"/>
    <match lemma="a" tags="pr"><select lemma="de" tags="pr"/></match>
  </rule>

  <!-- in >> en, a -->
  <rule weight="0.6">
    <match lemma="in" tags="pr"><select lemma="en"/></match>
  </rule>

  <rule weight="1.0">
    <or>
      <match lemma="arrivare" tags="vblex.*"/>
      <match lemma="venire" tags="vblex.*"/>
    </or>
    <repeat from="0" upto="1">
      <match tags="adv"/>
    </repeat>
    <match lemma="in" tags="pr"><select lemma="a"/></match>
  </rule>

  <rule weight="1.0">
    <match lemma="in" tags="pr"><select lemma="a"/></match>
    <repeat from="0" upto="1">
      <match tags="det.def.*"/>
    </repeat>
    <or>
      <match lemma="Catalogna"/>
      <match lemma="Aragona"/>
      <match lemma="Castiglia"/>
      <match lemma="Andalusia"/>
