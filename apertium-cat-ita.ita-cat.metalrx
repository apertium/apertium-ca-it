  <rules>

<!-- **************** DET **************** -->

  <!-- un altro > un altre, altre -->
  <rule weight="0.6">
    <match lemma="un altro" tags="det.*"><select lemma="un altre" tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="un altro" tags="det.ind.m.pl"><select lemma="altre" tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="un altro" tags="det.ind.f.pl"><select lemma="altre" tags="det.*"/></match>
  </rule>

  <!-- alcuno > un altre, cap -->
  <rule weight="0.6">
    <match lemma="alcun" tags="det.*"><select lemma="algun" tags="det.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="senza" tags="pr"/>
    <match lemma="alcun" tags="det.*"><select lemma="cap" tags="det.*"/></match>
  </rule>
  <rule weight="1.0" c="non c'è alcuna fretta">
    <match lemma="non" tags="adv"/>
     <repeat from="0" upto="2">
       <or>
         <match tags="prn.pro.*"/>
         <match tags="adv"/>
       </or>
     </repeat>
    <or><match tags="vblex.*"/><match tags="vbmod.*"/><match tags="vbhaver.*"/><match tags="vbser.*"/></or>
    <match lemma="alcun" tags="det.*"><select lemma="cap" tags="det.*"/></match>
  </rule>

<!-- **************** PRN **************** -->

  <!-- un altro > un altre, altre -->
  <rule weight="0.6">
    <match lemma="un altro" tags="prn.*"><select lemma="un altre" tags="prn.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="un altro" tags="prn.ind.m.pl"><select lemma="altre" tags="prn.*"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="un altro" tags="prn.ind.f.pl"><select lemma="altre" tags="prn.*"/></match>
  </rule>

  <!-- loro > ell, els, los -->
  <rule weight="0.6">
     <match lemma="lui" tags="prn.tn.p3.mf.*"><select lemma="ell" tags="prn.tn.p3.*"/></match>
  </rule>
  <rule weight="1.0">
     <match tags="vblex.*"/>
     <match lemma="lui" tags="prn.tn.p3.mf.pl"><select lemma="li" tags="prn.pro.p3.mf.pl"/></match>
  </rule>
  <rule weight="1.2">
     <or><match tags="vblex.inf"/><match tags="vblex.ger"/></or>
     <match lemma="lui" tags="prn.tn.p3.mf.pl"><select lemma="li" tags="prn.enc.p3.mf.pl"/></match>
  </rule>

<!-- **************** ADJ **************** -->

  <!-- ampio > ample, ampli -->
  <rule weight="0.6">
    <match lemma="ampio" tags="adj.*"><select lemma="ample"/></match>
  </rule>

  <!-- sinistro > esquerre, sinistre -->
  <rule weight="0.6">
    <match lemma="sinistro" tags="adj.*"><select lemma="esquerre"/></match>
  </rule>

  <!-- intero > sencer, enter -->
  <rule weight="0.6">
    <match lemma="intero" tags="adj.*"><select lemma="sencer"/></match>
  </rule>

  <!-- mercantile > mercantil, mercant -->
  <rule weight="0.6">
    <match lemma="mercantile" tags="adj.*"><select lemma="mercantil"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="marina" tags="n.*"/>
    <match lemma="mercantile" tags="adj.*"><select lemma="mercant"/></match>
  </rule>

  <!-- democratico > democràtic, demòcrata -->
  <rule weight="0.6">
    <match lemma="democratico" tags="adj.*"><select lemma="democràtic"/></match>
  </rule>

  <!-- rosso > vermell, negre -->
  <rule weight="0.6">
    <match lemma="rosso" tags="adj.*"><select lemma="vermell"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="vino" tags="n.*"/>
    <match lemma="rosso" tags="adj.*"><select lemma="negre"/></match>
  </rule>

  <!-- caro > car, benvolgut -->
  <rule weight="0.6">
    <match lemma="caro" tags="adj.*"><select lemma="car"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="caro" tags="adj.*"><select lemma="benvolgut"/></match>
    <match lemma="amico" tags="n.*"/>
  </rule>

<!-- **************** ADV **************** -->


<!-- **************** N **************** -->

  <!-- pollice > polze, polzada -->
  <rule weight="0.6">
    <match lemma="pollice" tags="n.*"><select lemma="polze"/></match>
  </rule>
  <rule weight="1.0">
     <or><match tags="num"/><match tags="num.*"/></or>
     <match lemma="pollice" tags="n.m.pl"><select lemma="polzada"/></match>
  </rule>

  <!-- tratto > tret, tram, traç, tracte -->
  <rule weight="0.6">
    <match lemma="tratto" tags="n.*"><select lemma="tret"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="tratto" tags="n.*"><select lemma="tracte"/></match>
    <or>
      <match lemma="respiratorio" tags="adj.*"/>
      <match lemma="gastrointestinale" tags="adj.*"/>
      <match lemma="genitourinario" tags="adj.*"/>
    </or>
  </rule>
  <rule weight="1.0">
    <match lemma="tratto" tags="n.*"><select lemma="tram"/></match>
    <match lemma="di" tags="pr"/>
    <repeat from="0" upto="1">
      <match tags="det.*"/>
    </repeat>
    <or>
      <match lemma="strada" tags="n.*"/>
      <match lemma="tangenziale" tags="n.*"/>
      <match lemma="via" tags="n.*"/>
      <match lemma="viale" tags="n.*"/>
      <match lemma="corso" tags="n.*"/>
    </or>
  </rule>

  <!-- rapporto > relació, informe -->
  <rule weight="0.6">
    <match lemma="rapporto" tags="n.*"><select lemma="relació"/></match>
  </rule>

  <!-- licenzia > llicència, permís -->
  <rule weight="0.6">
    <match lemma="licenzia" tags="n.*"><select lemma="llicència"/></match>
  </rule>

  <!-- vocabolario > vocabulari, diccionari -->
  <rule weight="0.6">
    <match lemma="vocabolario" tags="n.*"><select lemma="vocabulari"/></match>
  </rule>

  <!-- significato > significat, significació -->
  <rule weight="0.6">
    <match lemma="significato" tags="n.*"><select lemma="significat"/></match>
  </rule>

  <!-- utilizzo > ús, utilització -->
  <rule weight="0.6">
    <match lemma="utilizzo" tags="n.*"><select lemma="ús"/></match>
  </rule>

  <!-- opera > obra, opera -->
  <rule weight="0.6">
    <match lemma="opera" tags="n.*"><select lemma="obra"/></match>
  </rule>

  <!-- seno > si, pit, sinus -->
  <rule weight="0.6">
    <match lemma="seno" tags="n.*"><select lemma="si"/></match>
  </rule>
  <rule weight="1.0">
    <match tags="adj.*"/>
    <match lemma="seno" tags="n.*"><select lemma="pit"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="seno" tags="n.*"><select lemma="pit"/></match>
    <match tags="adj.*"/>
  </rule>
  <rule weight="1.0">
    <match lemma="seno" tags="n.*"><select lemma="sinus"/></match>
    <match lemma="di" tags="pr"/>
    <match tags="num"/>
  </rule>
  <rule weight="1.0">
    <match lemma="seno" tags="n.*"><select lemma="sinus"/></match>
    <match tags="cnjcoo"/>
    <match lemma="coseno" tags="n.*"/>
  </rule>

  <!-- giornata > jornada, jornal -->
  <rule weight="0.6">
    <match lemma="giornata" tags="n.*"><select lemma="jornada"/></match>
  </rule>

  <!-- conflitto > conflicte, altercat -->
  <rule weight="0.6">
    <match lemma="conflitto" tags="n.*"><select lemma="conflicte"/></match>
  </rule>

  <!-- coro > cor, coral -->
  <rule weight="0.6">
    <match lemma="coro" tags="n.*"><select lemma="cor"/></match>
  </rule>

  <!-- dimostrazione > manifestació, demostració -->
  <rule weight="0.6">
    <match lemma="dimostrazione" tags="n.*"><select lemma="manifestació"/></match>
  </rule>

  <!-- partita > partit, partida -->
  <rule weight="0.6">
    <match lemma="partita" tags="n.*"><select lemma="partit"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="partita" tags="n.*"><select lemma="partida"/></match>
    <match tags="pr"/>
    <or>
      <match lemma="carta" tags="n.*"/>
      <match lemma="scacchi" tags="n.*"/>
      <match lemma="domino" tags="n.*"/>
    </or>
  </rule>

  <!-- scherzo > broma, scherzo -->
  <rule weight="0.6">
    <match lemma="scherzo" tags="n.*"><select lemma="broma"/></match>
  </rule>
  <rule weight="1.0">
    <match lemma="scherzo" tags="n.*"><select lemma="scherzo"/></match>
    <match lemma="di" tags="pr"/>
    <match tags="det.*"/>
    <or>
      <match lemma="sinfonia" tags="n.*"/>
      <match lemma="concerto" tags="n.*"/>
      <match lemma="sonata" tags="n.*"/>
    </or>
  </rule>

